

<%= form_for @question, url: {action: "create"} do |f| -%>
  <%= f.label :form_title, "Create Question Form" %><br />
  <%= f.text_field :title, placeholder: "Question" %><br />
  <%= f.text_area :content, size: "40x12", placeholder: "Details" %><br />
  <%= f.submit "Create Question" %>
<% end -%>
